{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "civjs-server",
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}