{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "civjs-server",
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci",
  "functions": {
    "dist/index.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}